{
    "name": "simplemap/platform",
    "type": "project",
    "description": "Administration tool for your organization projects, with an intuitive control panel, team accounts, and more.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/RentadroneCL/AI-Solar-Platform/issues",
        "source": "https://github.com/RentadroneCL/AI-Solar-Platform"
    },
    "authors": [
        {
            "name": "Alvaro Farias",
            "email": "alvaro.farias@simplemap.io",
            "homepage": "https://simplemap.io",
            "role": "Lead programmer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-exif": "*",
        "aws/aws-sdk-php": "^3.176",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.8",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "league/csv": "^9.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.5",
        "miljar/php-exif": "^0.6.5",
        "rappasoft/laravel-livewire-tables": "^2.8",
        "spatie/laravel-db-snapshots": "^2.3",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "brianium/paratest": "^6.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^0.1.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^1.2",
        "php-coveralls/php-coveralls": "^2.5",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
